MAILDIR=$HOME/.mail      # You'd better make sure it exists
DEFAULT=$MAILDIR/mbox
LOGFILE=$MAILDIR/from

:0b
#* ^From.*jupyter@notebooks.hpedev.io.*
# \/ defines what will be matched in $MATCH
* ^Subject: *CREATE \/[1-9]+.*
| $HOME/procmail-action.sh CREATE $MATCH

:0b
#* ^From.*jupyter@notebooks.hpedev.io.*
# \/ defines what will be matched in $MATCH
* ^Subject: *CLEANUP \/[1-9]+.*
| $HOME/procmail-action.sh CLEANUP $MATCH
